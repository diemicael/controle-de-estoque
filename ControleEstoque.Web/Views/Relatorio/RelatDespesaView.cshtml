@model List<ControleEstoque.Web.Models.DespesaModel>
@{
	Layout = "";
}
<!DOCTYPE html>
<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>Posição no Estoque</title>
	@Styles.Render("~/Content/css")
	<style>

	</style>
</head>
<body>
	<div class="col-md-12 text-center">
		<img src="~/Content/Imagens/LOGO-TRASNPV2.png" style="height: 80px" />
		<h3>Despesas</h3>
	</div>
	<h3>Despesas</h3>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Data</th>
				<th>Água</th>
				<th>Internet</th>
				<th>Aluguel</th>
				<th>Segurança</th>
				<th>Telefone</th>
				<th>Marketing</th>
				<th>Salários</th>
				<th>Total</th>
			</tr>
		</thead>
		@{
			foreach (var item in Model)
			{
		<tr>
			<td>@item.Data</td>
			<td>@item.Agua</td>
			<td>@item.Internet</td>
			<td>@item.Aluguel</td>
			<td>@item.Seguranca</td>
			<td>@item.Telefone</td>
			<td>@item.Marketing</td>
			<td>@item.Salarios</td>
			<td>@item.Total</td>
		</tr>
			}
		}
	</table>
</body>
</html>

